      "update-template": true,
      "single-template": {
        "level0/level1[*]/level2[*]/level3/level4[?]/level4-02": ".*" ,
        "level0/level1[*]/level2[*]/level3/level4[?]/level4-03": ".*"
      },
      "single-template-count": {
        "level0/level1[*]/level2[*]" : 1,
        "level0/level1[*]" : 1

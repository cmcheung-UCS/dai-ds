      "default" : "old-ras",
      "types" :[
        "fabric-crit", "fabric-perf", "old-ras"
      ]
    },

    "sensor" : {
      "default" : "energy",
      "types" :[
        "energy", "fabric-perf", "power", "temperature", "voltage"
